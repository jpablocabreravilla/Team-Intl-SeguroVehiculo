@page
@model TGL.WebAppTeamSeguros.Pages.Vehicles.AddModel
@{
    ViewData["Title"] = "Add";
}


<nav>
    <div class="nav-wrapper bg-dark">
        <a class="brand-logo center">Add Vehicle</a>
    </div>
</nav>

<div class="row">
    <form method="post" class="col s12">
        <input hidden asp-for="CustomerId" />
        <div class="row">
            <div class="input-field col s12">
                <label for="name" class="">Brand</label>
                <input asp-for="Vehicle.Brand" type="text" />
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <label for="name" class="">Model</label>
                <input asp-for="Vehicle.Model" type="text" />
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <label for="numdoc" class="">Year</label>
                <input asp-for="Vehicle.Year" type="number" />
            </div>
        </div>
        <button class="btn waves-effect waves-light" type="submit" name="action">
            Save
            <i class="material-icons right">send</i>
        </button>
    </form>
</div>
<br />

